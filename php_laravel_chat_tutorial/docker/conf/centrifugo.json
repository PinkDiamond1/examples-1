{
  "port": 8000,
  "engine": "memory",
  "token_hmac_secret_key": "secret",
  "api_key": "some-key",
  "allowed_origins": [
    "http://localhost",
    "http://127.0.0.1"
  ],
  "proxy_connect_endpoint": "http://nginx/centrifugo/connect/",
  "proxy_http_headers": [
    "Cookie"
  ],
  "user_subscribe_to_personal": true,
  "user_personal_channel_namespace": "personal",
  "namespaces": [
    {
      "name": "personal",
      "presence": true
    }
  ]
}